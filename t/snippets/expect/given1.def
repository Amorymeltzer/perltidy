        given ( [ 9, "a", 11 ] ) {
            when (qr/\d/) {
                given ($count) {
                    when (1)          { ok( $count == 1 ) }
                    else              { ok( $count != 1 ) }
                    when ( [ 5, 6 ] ) { ok(0) }
                    else              { ok(1) }
                }
            }
            ok(1) when 11;
        }
